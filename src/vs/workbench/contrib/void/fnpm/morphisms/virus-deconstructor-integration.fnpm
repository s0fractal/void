  analyzeGeneQuality: |
    function(manifest) {
      console.log('\nðŸ§¬ Analyzing extracted genes...');
      
      // Parse NDJSON lines
      const genes = manifest.split('\n')
        .filter(line => line.trim())
        .map(line => JSON.parse(line));
      
      const stats = {
        total: genes.length,
        byType: {},
        avgComplexity: 0,
        purityScore: 100
      };
      
      // Categorize genes
      genes.forEach(gene => {
        const category = categorizeGene(gene);
        stats.byType[category] = (stats.byType[category] || 0) + 1;
      });
      
      console.log(`\nðŸ“Š Gene Analysis:`);
